{
  "$schema" : "http://json-schema.org/draft-07/schema#",
  "type" : "object",
  "properties" : {
    "comprobantes" : {
      "minItems" : 1,
      "type" : "array",
      "items" : {
        "type" : "object",
        "properties" : {
          "descripcionSustento" : {
            "type" : "string",
            "description" : "Descripción de la razón de baja"
          },
          "numero" : {
            "type" : "integer",
            "minimum" : 1,
            "maximum" : 99999999
          },
          "serie" : {
            "type" : "string"
          },
          "tipoComprobante" : {
            "type" : "string",
            "description" : "Catalogo 01"
          }
        }
      }
    },
    "fechaEmision" : {
      "type" : "string",
      "description" : "Format: \"YYYY-MM-SS\". Ejemplo: 2022-12-25",
      "pattern" : "^\\d{4}-\\d{2}-\\d{2}$"
    },
    "fechaEmisionComprobantes" : {
      "type" : "string",
      "description" : "Format: \"YYYY-MM-SS\". Ejemplo: 2022-12-25",
      "pattern" : "^\\d{4}-\\d{2}-\\d{2}$"
    },
    "firmante" : {
      "type" : "object",
      "properties" : {
        "razonSocial" : {
          "type" : "string"
        },
        "ruc" : {
          "type" : "string",
          "minLength" : 11,
          "maxLength" : 11,
          "pattern" : "[0-9]+"
        }
      }
    },
    "moneda" : {
      "type" : "string",
      "minLength" : 3,
      "maxLength" : 3
    },
    "numero" : {
      "type" : "integer",
      "minimum" : 1,
      "maximum" : 99999999
    },
    "proveedor" : {
      "type" : "object",
      "properties" : {
        "contacto" : {
          "type" : "object",
          "properties" : {
            "email" : {
              "type" : "string",
              "description" : "Correo electrónico"
            },
            "telefono" : {
              "type" : "string",
              "description" : "Número telefónico"
            }
          }
        },
        "direccion" : {
          "type" : "object",
          "properties" : {
            "codigoLocal" : {
              "type" : "string",
              "description" : "Código de cuatro dígitos asignado por SUNAT, que identifica al establecimiento anexo. Dicho código se genera al momento la respectiva comunicación del establecimiento. Tratándose del domicilio fiscal y en el caso de no poder determinar el lugar de la venta, informar \"0000\"",
              "minLength" : 4,
              "maxLength" : 4,
              "pattern" : "[0-9]+"
            },
            "codigoPais" : {
              "type" : "string",
              "description" : "Código de 2 dígitos que corresponde al país a la que pertenece la dirección. Ejemplo: PE",
              "minLength" : 2,
              "maxLength" : 2
            },
            "departamento" : {
              "type" : "string",
              "description" : "Nombre del Departamento o Región. Ejemplo: Ayacucho"
            },
            "direccion" : {
              "type" : "string",
              "description" : "Dirección. Ejemplo: Jirón las piedras 123"
            },
            "distrito" : {
              "type" : "string",
              "description" : "Nombre del Distrito. Ejemplo: Quinua"
            },
            "provincia" : {
              "type" : "string",
              "description" : "Nombre de la Provincia. Ejemplo: Huamanga"
            },
            "ubigeo" : {
              "type" : "string",
              "description" : "Código de 6 dígitos que corresponden al Departamento, Provincia, y Distrito",
              "minLength" : 6,
              "maxLength" : 6,
              "pattern" : "[0-9]+"
            },
            "urbanizacion" : {
              "type" : "string",
              "description" : "Nombre de la urbanización."
            }
          }
        },
        "nombreComercial" : {
          "type" : "string"
        },
        "razonSocial" : {
          "type" : "string"
        },
        "ruc" : {
          "type" : "string",
          "minLength" : 11,
          "maxLength" : 11,
          "pattern" : "[0-9]+"
        }
      }
    }
  }
}